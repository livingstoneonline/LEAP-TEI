	<!--<milestone unit="column">--><cb n="3"/>
<lb facs="#zliv_000096_0004_5_8"/><p><placeName><region>Mazimba</region></placeName> East</p>
	<!--<milestone unit="column">--><cb/>

	<pb facs="liv_000096_0003" n="0006"/><!--<milestone unit="page" n="6"/>-->
